{
    "builds": [{
        "src": "academia_musica/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "academia_musica/wsgi.py"
        }
    ],
    "scripts": {
      "postinstall": "python3.9 manage.py migrate"
    }
  }
  